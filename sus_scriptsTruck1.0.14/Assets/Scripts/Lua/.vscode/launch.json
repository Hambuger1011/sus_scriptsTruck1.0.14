    "version": "0.2.0",
    "configurations": [
    {
        "type": "emmylua_attach",
        "request": "attach",
        "name": "Attach by process id",
        "pid": 0
    },
        {
            "type": "emmylua_new",
            "request": "launch",
            "name": "EmmyLua New Debug",
            "host": "localhost",
            "port": 9966,
            "ext": [
                ".lua",
                ".lua.txt",
                ".lua.bytes"
            ],
            "ideConnectDebugger": true
        },
        
        {
            "name": "Unity-Remote",
            "type": "lua",
            "request": "attach",
            "runtimeType": "Unity",
            "localRoot": "${workspaceRoot}",
            "port": 7003,
            "printType": 1
        }

    ]
}